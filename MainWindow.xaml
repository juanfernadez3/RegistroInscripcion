<Window x:Class="RegistroInscripcion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegistroInscripcion"
        mc:Ignorable="d"
        Title="Menú" Height="300" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>

        <Menu Name="BarraMenu" IsMainMenu="True">
            <MenuItem Name="MenuItemPersonas" Header="Personas" FontFamily="Times New Roman" FontSize="20" Click="MenuItemPersonas_Click">

            </MenuItem>
            <MenuItem Name="MenuItemInscripcion" Header="Inscripciones"  FontFamily="Times New Roman" FontSize="20" Click="MenuItemInscripcion_Click">
            </MenuItem>
        </Menu>

    </Grid>
</Window>
